{
  "name": "ksa-site-backend",
  "version": "1.0.0",
  "description": "ksa site backend",
  "main": "app.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "database:migrate": "node -r dotenv/config node_modules/knex/bin/cli.js --cwd database/ migrate:latest",
    "database:rollback": "node -r dotenv/config node_modules/knex/bin/cli.js --cwd database/ migrate:rollback",
    "database:seed": "node -r dotenv/config node_modules/knex/bin/cli.js knex --cwd database/ seed:run",
    "lint": "eslint .",
    "start": "nodemon --config nodemon.json --require dotenv/config app.ts "
  },
  "author": "Maarten Vercammen",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.0",
    "cgi-express": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "knex-stringcase": "^1.4.6",
    "multer": "^1.4.5-lts.1",
    "node-fastcgi": "^1.3.3",
    "pg": "^8.7.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest": "^1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.0",
    "typescript": "^4.5.5"
  }
}
